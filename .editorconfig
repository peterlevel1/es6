# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
end_of_line = lf
insert_final_newline = false
trim_trailing_whitespace = true

# Set default charset
charset = utf-8

# 2 space indentation
indent_style = space
indent_size = 2

# max line length
max_line_length = 120
